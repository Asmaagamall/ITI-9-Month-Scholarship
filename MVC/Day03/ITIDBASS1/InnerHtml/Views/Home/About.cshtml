@{
    ViewBag.Title = "About";
}

@helper Getlst()
{
    var emps = InnerHtml.Models.EmpList.Employees;
    <ul>
        @foreach (var item in emps)
        {
        <li>@item.Id</li>
        <li>@item.Name</li>
        <li>@item.Salary</li>
        }
    </ul>
}
<main aria-labelledby="title">
    <h2 id="title">@ViewBag.Title.</h2>
    <h3>@ViewBag.Message</h3>
    <p>
        @Getlst();
    </p>

    <p>Use this area to provide additional information.</p>
</main>